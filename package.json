{
  "name": "superset-github-bot",
  "version": "1.0.0",
  "private": true,
  "description": "A Probot app",
  "author": "Maxime Beauchemin <maximebeauchemin@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/mistercrunch/superset-github-bot.git",
  "homepage": "https://github.com/mistercrunch/superset-github-bot",
  "bugs": "https://github.com/mistercrunch/superset-github-bot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./src/index.js",
    "lint": "semistandard --fix",
    "test": "jest && semistandard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "^12.3.0",
    "smee-client": "^1.1.0"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nock": "^11.4.0",
    "nodemon": "^1.19.4",
    "semistandard": "^16.0.0",
    "standard": "^16.0.3"
  },
  "engines": {
    "node": "8.11.0"
  },
  "semistandard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
